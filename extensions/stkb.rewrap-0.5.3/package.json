{
	"name": "rewrap",
	"displayName": "Rewrap",
	"description": "Re-wraps comments and other text to a given line length.",
	"keywords": [
		"wraps",
		"comments",
		"column",
		"length",
		"fill"
	],
	"version": "0.5.3",
	"publisher": "stkb",
	"icon": "images/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/stkb/vscode-rewrap.git"
	},
	"bugs": {
		"url": "https://github.com/stkb/vscode-rewrap/issues",
		"email": "_steve_@outlook.com"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^0.10.8"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rewrap.rewrapComment"
	],
	"main": "./out/src/Main",
	"contributes": {
		"commands": [
			{
				"command": "rewrap.rewrapComment",
				"title": "Rewrap Comment / Text"
			}
		],
		"configuration": {
			"title": "Rewrap configuration",
			"properties": {
				"rewrap.wrappingColumn": {
					"type": "number",
					"default": 0,
					"description": "Controls after how many characters comments are wrapped."
				}
			}
		},
		"keybindings": [
			{
				"command": "rewrap.rewrapComment",
				"key": "ctrl+k ctrl+w",
				"mac": "cmd+k cmd+w",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"mocha-clean": "^1.0.0",
		"mz": "^2.4.0",
		"typescript": "^1.8.10",
		"vscode": "^0.11.10"
	},
	"dependencies": {
		"greedy-wrap": "^0.1.4",
		"markdown-to-ast": "^3.2.3"
	},
	"__metadata": {
		"id": "b24a13f5-4e50-4d9a-ab0a-87f47d9fdfcb",
		"publisherId": "30dfa722-ba55-4140-a1ba-4dd768bc0f57",
		"publisherDisplayName": "stkb"
	}
}